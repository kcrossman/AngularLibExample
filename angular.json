{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "open-ng-lib": {
      "projectType": "library",
      "root": "projects/open-ng-lib",
      "sourceRoot": "projects/open-ng-lib/src",
      "prefix": "onl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/open-ng-lib/tsconfig.lib.json",
            "project": "projects/open-ng-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/open-ng-lib/src/test.ts",
            "tsConfig": "projects/open-ng-lib/tsconfig.spec.json",
            "karmaConfig": "projects/open-ng-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/open-ng-lib/tsconfig.lib.json",
              "projects/open-ng-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "open-ng-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/open-ng-docs",
      "sourceRoot": "projects/open-ng-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/open-ng-docs",
            "index": "projects/open-ng-docs/src/index.html",
            "main": "projects/open-ng-docs/src/main.ts",
            "polyfills": "projects/open-ng-docs/src/polyfills.ts",
            "tsConfig": "projects/open-ng-docs/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/open-ng-docs/src/favicon.ico",
              "projects/open-ng-docs/src/assets"
            ],
            "styles": [
              "projects/open-ng-docs/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "projects/open-ng-docs/src/environments/environment.ts",
                "with": "projects/open-ng-docs/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "open-ng-docs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "open-ng-docs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "open-ng-docs:build"
          }
        }
      }
    }
  },
  "defaultProject": "open-ng-lib"
}
